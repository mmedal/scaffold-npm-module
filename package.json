{
  "name": "{% name %}",
  "version": "0.0.0",
  "description": "{% description %}",
  "author": "{% author %}",
  "license": "MIT",
  "repository": "git@github.com:{% owner %}/{% name %}.git",
  "homepage": "https://github.com/{% owner %}/{% name %}",
  "bugs": "https://github.com/{% owner %}/{% name %}/issues",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],{% public? %}{% else %}
  "private": true,{% end %}
  "scripts": {
    "clean": "shx rm -rf lib/ && shx mkdir lib/",
    "prebuild": "npm run clean",
    "build": "babel --copy-files --out-dir lib/ src/",
    "preversion": "npm run test",
    "postversion": "npm-publish-safe-latest && git push --follow-tags",
    "test": "npm run build && node lib/test/index.js"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "npm-publish-safe-latest": "^1.1.2",
    "shx": "^0.1.2",
    "tape": "^4.6.0"
  }
}
